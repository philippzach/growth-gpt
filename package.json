{
  "name": "growth-strategy-agent-system",
  "version": "1.0.0",
  "description": "Interactive Growth Strategy AI Agent System with 8 specialized agents",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "tsc && vite build",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx,json}",
    "frontend:dev": "vite",
    "frontend:build": "vite build",
    "frontend:preview": "vite preview",
    "worker:dev": "wrangler dev",
    "worker:deploy": "wrangler deploy",
    "worker:tail": "wrangler tail",
    "kv:create": "wrangler kv:namespace create CONFIG_STORE && wrangler kv:namespace create SESSION_STORE"
  },
  "keywords": [
    "growth-strategy",
    "ai-agents",
    "cloudflare-workers",
    "claude-api",
    "marketing-strategy"
  ],
  "author": "Philip Zach",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@headlessui/react": "^1.7.17",
    "@supabase/supabase-js": "^2.39.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "hono": "^4.0.0",
    "lucide-react": "^0.300.0",
    "nunjucks": "^3.2.4",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.20.0",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^3.4.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "wrangler": "^3.19.0"
  },
  "engines": {
    "node": ">=18.0.0"
