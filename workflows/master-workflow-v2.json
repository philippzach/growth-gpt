{
  "schema_version": "2.0",
  "description": "Complete growth and marketing strategy development workflow using standardized agent orchestration",
  "workflow": {
    "id": "master-workflow-v2",
    "name": "Growth and Marketing Strategy Development",
    "version": "2.0",
    "description": "End-to-end agent workflow for building comprehensive growth and marketing strategies.\nSupports both detailed strategic planning for complex projects and rapid prototyping for MVPs.\nIntegrates 8 specialized agents in sequential execution with quality gates and validation.\n",
    "type": "master-workflow",
    "category": "strategy",
    "project_types": [
      "growth-strategy",
      "marketing-strategy",
      "product-strategy",
      "go-to-market-strategy",
      "mvp-strategy",
      "startup-strategy"
    ],
    "complexity_level": "complex",
    "estimated_duration": "4-8 hours",
    "user_involvement": "medium"
  },
  "execution": {
    "mode": "sequential",
    "parallelization": {
      "max_concurrent_agents": 1,
      "parallel_groups": [],
      "synchronization_points": [
        "foundation_complete",
        "strategy_complete",
        "validation_complete"
      ]
    },
    "error_handling": {
      "strategy": "retry_failed",
      "max_retries": 2,
      "rollback_on_failure": true
    },
    "state_management": {
      "persist_state": true,
      "checkpoint_frequency": "per_agent",
      "resume_capability": true
    }
  },
