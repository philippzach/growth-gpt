      "inputs": {
        "required": [],
        "optional": [
          "initial_business_concept",
          "market_research_data"
        ],
        "user_inputs": [
          "business_idea",
          "target_market_hypothesis",
          "problem_statement"
        ],
        "context_injection": false
      },
      "outputs": {
        "primary": "gtm-brief.md",
        "secondary": [],
        "format": "markdown",
        "storage_location": "docs/"
      },
      "agent_overrides": {
        "temperature": 0.7,
        "max_tokens": 4000,
        "custom_prompt_additions": "Focus on creating a solid foundation for all subsequent agents."
      },
      "validation": {
        "required_elements": [
          "value_proposition",
          "target_market",
          "problem_validation",
          "business_model_overview"
